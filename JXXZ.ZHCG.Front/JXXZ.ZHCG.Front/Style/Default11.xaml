        <GradientStop Color="#00a37c"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="RadPager_Page_Checked" EndPoint="0.5,0.99" StartPoint="0.5,0.01">
        <GradientStop Color="#00a37c" Offset="0.5"/>
        <GradientStop Color="#00a37c"/>
